SBEND 1.78931916 0.1308996937 0 0 0 0 0 0.5 0.5 ; {BM}
DL 0.13 ; {O5}
DL 0.3 ; {Sextupole}
MH LEN_S_1 SF1 0.05; {MCS1}
QUAD 0.2 0 QF ; {QF}
QUAD 0.2 0 QF ; {QF}
DL 0.09 ; {O3}
SBEND 1.78931916 0.1308996937 0 0 0 0 0 0.5 0.5 ; {BM}
DL 0.13 ; {O5}
DL 0.3 ; {Sextupole}
MH LEN_S_1 SD 0.05; {MCS3}
QUAD 0.2 0 QD ; {QD}
