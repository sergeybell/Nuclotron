QUAD 0.2 0 QF ; {QF}
DL 0.09 ; {O3}
SBEND 1.78931916 0.1308996937 0 0 0 0 0 0.5 0.5 ; {BM}
DL 0.43 ; {O4}
QUAD 0.2 0 QD ; {QD}
QUAD 0.2 0 QD ; {QD}

{========SS empty========== 132-123+1=10}
{WRITE 6 'Empty SS';
DL 3.35 ; {O1}
QUAD 0.2 0 QF1 ; {QF1}
QUAD 0.2 0 QF1 ; {QF1}
DL 3.35 ; {O1}
